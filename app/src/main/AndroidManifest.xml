<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.visk.android.stockmanager">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="com.visk.android.stockmanager.StockApplication"
        android:allowBackup="true"
        android:icon="@drawable/launch_ic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launch_ic"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomUserInfo">
        <activity android:name="com.visk.android.stockmanager.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>